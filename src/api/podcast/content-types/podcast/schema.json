{
  "kind": "collectionType",
  "collectionName": "podcasts",
  "info": {
    "singularName": "podcast",
    "pluralName": "podcasts",
    "displayName": "Podcast"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "featureImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "podcasts"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "inversedBy": "podcasts"
    },
    "mainSection": {
      "type": "dynamiczone",
      "components": [
        "blog-and-casestudies.tldr-section",
        "blog-and-casestudies.sepration",
        "blog-and-casestudies.blog-or-casestudy-text",
        "blog-and-casestudies.section-image",
        "shared.seo",
        "shared.rich-text",
        "blog-and-casestudies.video",
        "blog-and-casestudies.iframe",
        "blog-and-casestudies.code-section",
        "shared.quote"
      ]
    }
  }
}
