{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "featureImage": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "mainSection": {
      "type": "dynamiczone",
      "components": [
        "blog-and-casestudies.tldr-section",
        "blog-and-casestudies.sepration",
        "blog-and-casestudies.blog-or-casestudy-text",
        "blog-and-casestudies.section-image",
        "shared.seo",
        "shared.rich-text",
        "blog-and-casestudies.video",
        "blog-and-casestudies.iframe",
        "blog-and-casestudies.code-section"
      ]
    },
    "blogProfileSection": {
      "type": "component",
      "component": "shared.profile",
      "repeatable": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "blogs"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "blogs"
    }
  }
}
